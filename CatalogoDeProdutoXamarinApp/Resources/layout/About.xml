<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
 <LinearLayout
		android:orientation="vertical"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:id="@+id/linearLayout1">
		<ImageView
			android:layout_width="match_parent"
			android:layout_height="197.5dp"
			android:id="@+id/Logo" />
		<TextView
			android:text="product catalog"
			android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/lbl_CatalogoDeProduto"
			android:layout_gravity="center"
			android:gravity="center" />
		<TextView
			android:text="Developer"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/lbl_desenvolvedor"
			android:gravity="center"
			android:layout_gravity="center" />
		<TextView
			android:text="Adriano Severino"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/lbl_adriano"
			android:gravity="center"
			android:layout_gravity="center" />
		<TextView
			android:text="GitHub"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/lbl_github"
			android:gravity="center"
			android:layout_gravity="center"
			android:textColor="@android:color/holo_blue_dark" />
		<TextView
			android:text="Linkedin"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/lbl_linkedin"
			android:gravity="center"
			android:layout_gravity="center"
			android:textColor="@android:color/holo_blue_dark" />
		<TextView
			android:text="Tel:(11)97242-0472"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/lbl_tel"
			android:gravity="center"
			android:layout_gravity="center" />
        <TextView
            android:text="Version"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/lbl_versao"
            android:gravity="center"
            android:layout_gravity="center" />
		<LinearLayout
			android:orientation="vertical"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/linearLayout2"
			android:layout_gravity="center|bottom">
		</LinearLayout>
		<LinearLayout
			android:orientation="vertical"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="match_parent"
			android:layout_height="128.5dp"
			android:id="@+id/linearLayout3"
			android:layout_marginBottom="25.5dp">
			<LinearLayout
				android:orientation="vertical"
				android:minWidth="25px"
				android:minHeight="25px"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/linearLayout5">
			</LinearLayout>
			<TextView
				android:text="back"
				android:textAppearance="?android:attr/textAppearanceSmall"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/lbl_voltar"
				android:gravity="center"
				android:layout_gravity="center"
				android:foregroundGravity="center" />
		</LinearLayout>
		<LinearLayout
			android:orientation="vertical"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/linearLayout4">
			<LinearLayout
				android:orientation="vertical"
				android:minWidth="25px"
				android:minHeight="25px"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/linearLayout6">
				<TextView
					android:text="CopyrightÂ©2019 ASXadrianoSeverino"
					android:textAppearance="?android:attr/textAppearanceSmall"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/lbl_Copyright"
					android:gravity="center"
					android:layout_gravity="center"
					android:foregroundGravity="center" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
